{
  "name": "<%=props.express.expressName%>",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    <% if(props.express.expressDependencies.cors) { %>
    "cors": "^2.8.5",
    <% } %> 
    <% if(props.packageManager === 'pnpm' && props.express.expressDependencies.prisma) { %>
      "@repo/db": "workspace:*",
    <% } %> 
    <% if((props.packageManager === 'npm' || props.packageManager === 'yarn') && props.express.expressDependencies.prisma) { %>
      "@repo/db": "*",
    <% } %> 
    "dotenv": "^16.4.5",
    "express": "^4.21.0"
  },
  "devDependencies": {
    <% if(props.express.expressDependencies.cors) { %>
    "@types/cors": "^2.8.17",
    <% } %> 
    "@types/express": "^4.17.21",
    "tsup": "^8.1.0"
  }
}

