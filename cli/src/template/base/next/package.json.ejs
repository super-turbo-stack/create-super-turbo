{
  "name": "<%=props.next.nextName%>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    <% if(props.next.nextDependencies.nextAuth) { %>
    "next-auth": "^4.24.7",
    <% } %> 
    <% if(props.next.nextDependencies.recoil) { %>
    "recoil": "^0.7.7",
    <% } %> 
    <% if(props.next.nextDependencies.tanstackQuery) { %>
    "@tanstack/react-query": "^5.56.2",
    <% } %> 
    <% if(props.packageManager === 'pnpm' && props.next.nextDependencies.prisma) { %>
    "@repo/db": "workspace:*",
    <% } %> 
    <% if((props.packageManager === 'npm' || props.packageManager === 'yarn') && props.next.nextDependencies.prisma) { %>
    "@repo/db": "*",
    <% } %> 
    <% if(props.packageManager === 'pnpm' && props.next.nextDependencies.recoil) { %>
    "@repo/store": "workspace:*",
    <% } %> 
    <% if((props.packageManager === 'npm' || props.packageManager === 'yarn') && props.next.nextDependencies.recoil) { %>
    "@repo/store": "*",
    <% } %> 
    "next": "14.2.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    <% if(props.packageManager === 'npm' || props.packageManager === 'yarn') { %>
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    <% } %> 
    <% if(props.packageManager === 'pnpm') { %>
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    <% } %> 
    <% if(
        (props.next.nextDependencies.tailwind || props.next.nextDependencies.shadcnTailwind)
        && (props.packageManager === 'npm' || props.packageManager === 'yarn')
    ) { %>
        "@repo/tailwind-config": "*",
    <% } %> 
    <% if(
        (props.next.nextDependencies.tailwind || props.next.nextDependencies.shadcnTailwind)
        && (props.packageManager === 'pnpm')
    ) { %>
        "@repo/tailwind-config": "workspace:*",
    <% } %> 
    <% if(props.next.nextDependencies.tailwind || props.next.nextDependencies.shadcnTailwind) { %>
        "autoprefixer": "^10.4.18",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
    <% } %> 
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "typescript": "^5"
  }
}
