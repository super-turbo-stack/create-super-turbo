{
  "name": "<%=props.react.reactName%>",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    <% if(props.react.reactDependencies.reactRouter) { %>
    "react-router-dom": "^6.26.2",
    <% } %> 
    <% if(props.react.reactDependencies.recoil) { %>
    "recoil": "^0.7.7",
    <% } %> 
    <% if(props.react.reactDependencies.tanstackQuery) { %>
    "@tanstack/react-query": "^5.56.2",
    <% } %> 
    <% if(props.packageManager === 'pnpm' && props.react.reactDependencies.recoil) { %>
      "@repo/store": "workspace:*",
    <% } %> 
    <% if((props.packageManager === 'npm' || props.packageManager === 'yarn') && props.react.reactDependencies.recoil) { %>
      "@repo/store": "*",
    <% } %> 
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    <% if(props.packageManager === 'npm' || props.packageManager === 'yarn') { %>
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    <% } %> 
    <% if(props.packageManager === 'pnpm') { %>
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    <% } %> 
    <% if(
        (props.react.reactDependencies.tailwind || props.react.reactDependencies.shadcnTailwind)
        && (props.packageManager === 'npm' || props.packageManager === 'yarn')
    ) { %>
        "@repo/tailwind-config": "*",
    <% } %> 
    <% if(
        (props.react.reactDependencies.tailwind || props.react.reactDependencies.shadcnTailwind)
        && (props.packageManager === 'pnpm')
    ) { %>
        "@repo/tailwind-config": "workspace:*",
    <% } %> 
    <% if(props.react.reactDependencies.tailwind || props.react.reactDependencies.shadcnTailwind) { %>
        "autoprefixer": "^10.4.18",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
    <% } %> 
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
